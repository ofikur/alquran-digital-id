{
  "rewrites": [
    {
      "source": "/api/v2/:path*",
      "destination": "https://equran.id/api/v2/:path*"
    },
    {
      "source": "/api/:path*",
      "destination": "https://equran.id/api/:path*"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" }
  ]
}
