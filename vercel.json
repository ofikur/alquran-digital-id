{
  "rewrites": [
    {
      "source": "/api/v2/:path*",
      "destination": "https://equran.id/api/v2/:path*"
    },
    {
      "source": "/api/:path*",
      "destination": "https://equran.id/api/:path*"
    },
    {
      "source": "/((?!api/|_next/|static/|images/|favicon.ico).*)",
      "destination": "/index.html"
    }
  ]
}